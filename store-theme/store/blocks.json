    "product-details#default": {
        "blocks": [
            "breadcrumb",
            "product-name",
            "product-images",
            "product-price",
            "product-description",
            "product-specifications",
            "buy-button",
            "sku-selector",
            "shipping-simulator",
            "availability-subscriber",
